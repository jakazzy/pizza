  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jakazzy/pizza/issues"
  },
  "homepage": "https://github.com/jakazzy/pizza#readme",
  "dependencies": {
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "standard": "16.0.3",
    "tap-spec": "5.0.0",
    "tape": "5.2.2"
  }
}
